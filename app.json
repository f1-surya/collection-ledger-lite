{
  "expo": {
    "name": "Collection ledger",
    "slug": "collection-ledger",
    "version": "0.10.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "collectionledger",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "LSApplicationQueriesSchemes": ["whatsapp"]
      },
      "bundleIdentifier": "com.surya.collectionledger"
    },
    "android": {
      "packageName": "com.surya.collectionledger",
      "package": "com.surya.collectionledger",
      "permissions": [
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE",
        "DOWNLOADS"
      ]
    },
    "plugins": [
      "expo-router",
      "expo-build-properties",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-sqlite",
      [
        "react-native-share",
        {
          "ios": ["whatsapp"],
          "android": [""]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7ced4a7f-1214-4774-b5b1-4b822cf118ef"
      }
    },
    "owner": "f1-surya"
  }
}
